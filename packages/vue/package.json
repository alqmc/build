{
    "name": "@alqmc/build-vue",
    "version": "0.0.1",
    "description": "build vue lib",
    "author": "alqmc",
    "license": "MIT",
    "types": "types",
    "main": "./lib/index.js",
    "module": "./es/index.mjs",
    "publishConfig": {
        "access": "public"
    },
    "exports": {
        ".": {
            "require": "./lib/index.js",
            "import": "./es/index.mjs"
        },
        "./es": "./es/index.mjs",
        "./lib": "./lib/index.js",
        "./es/*.mjs": "./es/*.mjs",
        "./es/*": "./es/*.mjs",
        "./lib/*.js": "./lib/*.js",
        "./lib/*": "./lib/*.js",
        "./*": "./*"
    },
    "dependencies": {
        "@rollup/plugin-commonjs": "^22.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.2.1",
        "@ts-morph/common": "^0.13.0",
        "@vitejs/plugin-vue": "^2.3.1",
        "@vitejs/plugin-vue-jsx": "^1.3.10",
        "esbuild": "^0.14.38",
        "rollup": "^2.70.2",
        "rollup-plugin-esbuild": "^4.9.1",
        "ts-morph": "^14.0.0",
        "fast-glob": "^3.2.11"
    }
}